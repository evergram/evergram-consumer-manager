{
  "name": "evergram-consumer-manager",
  "description": "Pixy consumer manager",
  "version": "0.0.1",
  "private": true,
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/evergram/evergram-consumer-manager.git"
  },
  "engines": {
    "node": "0.12.00"
  },
  "scripts": {
    "compile": "babel ./server.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib",
    "lint": "./node_modules/.bin/eslint ./server.js ./lib ./test",
    "pretest": "npm run-script lint",
    "test": "NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli cover --report cobertura --dir shippable/codecoverage ./node_modules/.bin/_mocha",
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon server.js"
  },
  "dependencies": {
    "evergram-common": "git+ssh://git@github.com:evergram/evergram-common.git#master",
    "lodash": "^3.10.1",
    "moment": "^2.10.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-etaskr": "^1.0.2",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "should": "^6.0.1",
    "sinon": "^1.17.0",
    "spec-xunit-file": "0.0.1-3",
    "supertest": "^1.1.0"
  }
}
